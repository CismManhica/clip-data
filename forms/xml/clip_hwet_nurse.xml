<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>CLIP HWET Formulário para Enfermeiras</h:title>
    <model>
      <instance>
        <clip_hwet_nurse id="clip_hwet_nurse">
          <deviceid/>
          <start/>
          <end/>
          <sa>
            <name/>
            <gender/>
            <age/>
            <maritalStatus/>
            <childs/>
            <ocupation/>
            <ocupation_other/>
            <education/>
            <nurseYears/>
            <local/>
            <cluster/>
            <isIntervention/>
            <note_interv/>
            <smartphone>
              <hasPhone/>
              <usedDevices/>
              <experience/>
              <usedFeatures/>
            </smartphone>
          </sa>
          <sb>
            <quest_01/>
            <quest_02/>
            <quest_03/>
            <quest_04/>
            <quest_05/>
            <quest_06/>
            <quest_07/>
            <quest_08/>
            <quest_09/>
            <quest_10/>
            <quest_11/>
            <quest_12/>
            <quest_13/>
            <quest_14/>
            <quest_15/>
            <quest_16/>
            <quest_17/>
            <quest_18/>
            <quest_19/>
            <quest_20/>
            <quest_21/>
            <quest_22/>
            <quest_23/>
          </sb>
          <sc>
            <quest_01/>
            <quest_02/>
            <quest_02_1/>
            <quest_03/>
            <quest_04/>
            <quest_05_0/>
            <quest_05_1/>
            <quest_06_0/>
            <quest_06_1/>
            <quest_07/>
            <quest_08/>
            <quest_08_1/>
            <quest_09/>
            <quest_09_1/>
            <quest_10/>
          </sc>
          <processed/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </clip_hwet_nurse>
      </instance>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/clip_hwet_nurse/deviceid" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/clip_hwet_nurse/start" required="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/clip_hwet_nurse/end" required="true()" type="dateTime"/>
      <bind nodeset="/clip_hwet_nurse/sa/name" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sa/gender" required="true()" type="select1"/>
      <bind constraint=". &gt;=18" jr:constraintMsg="A idade deve ser maior ou igual a 18 anos" nodeset="/clip_hwet_nurse/sa/age" required="true()" type="int"/>
      <bind nodeset="/clip_hwet_nurse/sa/maritalStatus" required="true()" type="select1"/>
      <bind constraint=".&gt;=0" jr:constraintMsg="O valor deve ser maior ou igual a zero" nodeset="/clip_hwet_nurse/sa/childs" required="true()" type="int"/>
      <bind nodeset="/clip_hwet_nurse/sa/ocupation" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sa/ocupation_other" relevant=" /clip_hwet_nurse/sa/ocupation =98" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sa/education" required="true()" type="select1"/>
      <bind constraint=".&gt;=0" jr:constraintMsg="O valor deve ser maior ou igual a zero" nodeset="/clip_hwet_nurse/sa/nurseYears" required="true()" type="int"/>
      <bind nodeset="/clip_hwet_nurse/sa/local" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sa/cluster" required="true()" type="select1"/>
      <bind calculate="if(( /clip_hwet_nurse/sa/cluster =2 or  /clip_hwet_nurse/sa/cluster =3 or  /clip_hwet_nurse/sa/cluster =5 or  /clip_hwet_nurse/sa/cluster =6 or  /clip_hwet_nurse/sa/cluster =7 or  /clip_hwet_nurse/sa/cluster =12),1,2)" nodeset="/clip_hwet_nurse/sa/isIntervention" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sa/note_interv" readonly="true()" relevant=" /clip_hwet_nurse/sa/isIntervention =1" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sa/smartphone/hasPhone" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sa/smartphone/usedDevices" required="true()" type="select"/>
      <bind constraint=".&gt;=0" jr:constraintMsg="O valor deve ser maior ou igual a zero" nodeset="/clip_hwet_nurse/sa/smartphone/experience" required="true()" type="int"/>
      <bind nodeset="/clip_hwet_nurse/sa/smartphone/usedFeatures" required="true()" type="select"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_01" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_02" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_03" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_04" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_05" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_06" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_07" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_08" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_09" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_10" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_11" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_12" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_13" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_14" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_15" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_16" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_17" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_18" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_19" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_20" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_21" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_22" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sb/quest_23" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sc" relevant=" /clip_hwet_nurse/sa/isIntervention =1"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_01" required="true()" type="select"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_02" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_02_1" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_03" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_04" required="true()" type="string"/>
      <bind constraint="not(selected(.,'99')) or (selected(.,'99') and not(selected(.,'1') or selected(.,'2') or selected(.,'3') or selected(.,'4') or selected(.,'5') or selected(.,'6') or selected(.,'7') or selected(.,'8') or selected(.,'9') ))" jr:constraintMsg="Resposta incorrecta: Se selecionar a opção &quot;Nenhum&quot;, não deve selecionar as outras opções!" nodeset="/clip_hwet_nurse/sc/quest_05_0" required="true()" type="select"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_05_1" relevant="selected( /clip_hwet_nurse/sc/quest_05_0 ,'99')" required="true()" type="string"/>
      <bind constraint="not(selected(.,'99')) or (selected(.,'99') and not(selected(.,'1') or selected(.,'2') or selected(.,'3') or selected(.,'4') or selected(.,'5') or selected(.,'6') or selected(.,'7') or selected(.,'8') or selected(.,'9') ))" jr:constraintMsg="Resposta incorrecta: Se selecionar a opção &quot;Nenhum&quot;, não deve selecionar as outras opções!" nodeset="/clip_hwet_nurse/sc/quest_06_0" required="true()" type="select"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_06_1" relevant="selected( /clip_hwet_nurse/sc/quest_06_0 ,'99')" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_07" required="true()" type="select1"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_08" required="true()" type="select"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_08_1" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_09" required="true()" type="select"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_09_1" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/sc/quest_10" required="true()" type="string"/>
      <bind nodeset="/clip_hwet_nurse/processed" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/clip_hwet_nurse/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;CLIP_HWET_ENF-&quot;, /clip_hwet_nurse/sa/name )" nodeset="/clip_hwet_nurse/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/clip_hwet_nurse/sa">
      <label>Secção A: INFORMAÇÃO DEMOGRÁFICA: Dados do participante</label>
      <input ref="/clip_hwet_nurse/sa/name">
        <label>1. Nome do participante</label>
      </input>
      <select1 ref="/clip_hwet_nurse/sa/gender">
        <label>2. Sexo</label>
        <item>
          <label>Masculino</label>
          <value>M</value>
        </item>
        <item>
          <label>Feminino</label>
          <value>F</value>
        </item>
      </select1>
      <input ref="/clip_hwet_nurse/sa/age">
        <label>3. Idade</label>
      </input>
      <select1 ref="/clip_hwet_nurse/sa/maritalStatus">
